<%= form_with model: @board do |form| %>
  <%= form.text_field :title, placeholder: 'title' %>
  <%= form.text_field :appname, placeholder: 'appname' %>
  <%= form.text_field :description, placeholder: 'description' %>
  <%= form.text_field :website, placeholder: 'website' %>
  <%= form.submit %>
<% end %>
